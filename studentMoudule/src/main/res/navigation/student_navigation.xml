<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/student_navigation.xml"
    app:startDestination="@id/studentProfileFragment">

    <fragment
        android:id="@+id/studentProfileFragment"
        android:name="learn.atharv.studentmoudule.view.StudentProfileFragment"
        android:label="fragment_student_profile"
        tools:layout="@layout/fragment_student_profile" >
        <action
            android:id="@+id/action_studentProfileFragment_to_studentPaymentFragment"
            app:destination="@id/studentPaymentFragment" />
    </fragment>
    <fragment
        android:id="@+id/studentPaymentFragment"
        android:name="learn.atharv.studentmoudule.view.StudentPaymentFragment"
        android:label="fragment_student_payment"
        tools:layout="@layout/fragment_student_payment" >
        <action
            android:id="@+id/action_studentPaymentFragment_to_studentProfileFragment"
            app:destination="@id/studentProfileFragment" />
    </fragment>
</navigation>